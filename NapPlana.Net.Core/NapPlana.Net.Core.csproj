<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <AssemblyName>NapPlana.Net.Core</AssemblyName>
        <RootNamespace>NapPlana.Core</RootNamespace>
        <UserSecretsId>80185e78-85e8-4e72-bdd0-5d04bc13506f</UserSecretsId>
        <PackageId>NapPlana</PackageId>
        <Version>0.0.1</Version>
        <Title>NapPlana.NET</Title>
        <Authors>stevesensei</Authors>
        <Description>Yet Another NapCat Framework SDK for .NET</Description>
        <PackageProjectUrl>https://github.com/stevesensei/NapPlana.NET</PackageProjectUrl>
        <RepositoryUrl>https://github.com/stevesensei/NapPlana.NET</RepositoryUrl>
        <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    </PropertyGroup>

    <ItemGroup>
      <Folder Include="Connections\Http\" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="TouchSocket" Version="4.0.0-rc.5" />
      <PackageReference Include="TouchSocket.Http" Version="4.0.0-rc.5" />
    </ItemGroup>

</Project>
